#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_which@2.0.2@which/bin/node-which" "$@"
else 
  exec node  "$basedir/../_which@2.0.2@which/bin/node-which" "$@"
fi
